package seleniumquestons;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class LanuchWebpageUsingChromeDriver {
	public static void main(String[] args) 
	{
 
 // Open chromedriver
	System.setProperty("Dwebdriver.Chrome.driver","E:\\chromedriver.exe");
	WebDriver driver = new ChromeDriver();
	driver.get("http://learn-automation.com/how-to-capture-screenshot-in-selenium-webdriver/");
	}
}
